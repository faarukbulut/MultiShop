@model UserDetailViewModel
@{
    Layout = null;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<div class="container">
    <br /><br /><br>
    <h3>Kullanıcı TEST Profil Bilgisi</h3>
    <div class="alert alert-info">Kullanıcı Id: @Model.Id</div>
    <div class="alert alert-info">Kullanıcı Username: @Model.Username</div>
    <div class="alert alert-info">Kullanıcı Ad Soyad: @Model.Name @Model.Surname</div>
    <div class="alert alert-info">Kullanıcı Email: @Model.Email</div>
</div>
